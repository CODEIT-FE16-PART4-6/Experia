{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_BACKEND_URL": "@next_public_backend_url_dev",
      "NEXT_PUBLIC_TEAM": "@next_public_team_dev"
    }
  }
}
