{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "development",
    "NEXT_PUBLIC_BACKEND_URL": "https://api-dev.experia.com",
    "NEXT_PUBLIC_TEAM": "development"
  }
}
