{
  "name": "experia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "test": "jest",
    "ai-review": "node scripts/ai-review.js",
    "ai-review:openai": "node scripts/ai-review-openai.js",
    "commit": "AI_REVIEW=true git commit",
    "commit:ai": "AI_REVIEW=true git commit",
    "commit:ai:openai": "OPENAI_REVIEW=true git commit",
    "commit:normal": "AI_REVIEW=false git commit",
    "commit:interactive": "./scripts/commit-helper.sh",
    "vercel:dev": "./scripts/vercel-deploy.sh development",
    "vercel:staging": "./scripts/vercel-deploy.sh staging",
    "vercel:prod": "./scripts/vercel-deploy.sh production",
    "vercel:deploy": "./scripts/vercel-deploy.sh"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@hookform/resolvers": "^5.2.1",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@types/react-datepicker": "^6.2.0",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "next": "15.4.6",
    "nth-check": "^2.1.1",
    "photoswipe": "^5.4.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "react": "19.1.0",
    "react-datepicker": "^8.7.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-leaflet": "^5.0.0",
    "react-photoswipe-gallery": "^3.1.1",
    "swiper": "^11.2.10",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jira-prepare-commit-msg": "^1.7.2",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "webpack": "^5.101.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,css,scss,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "[$J] $M",
    "allowEmptyCommitMessage": false,
    "ignoredBranchesPattern": "^(master|main|dev|develop|development|release)$"
  }
}
